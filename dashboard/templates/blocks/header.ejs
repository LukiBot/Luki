<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- SEO -->
  <meta name="description" content="Luki is an easy-to-use bot with commands that can be used for fun, moderation, games, and more!">
  <meta name="keywords" content="MrSheldon, Luki, Luki The Bot, discord bot, discord music bot, discord bots, luki discord bot, MrSheldon#0001, Jonasjac, Ofir#0011 Olybear9, luki.js, JavaScript" />
  <meta name="author" content="MrSheldon">
  <meta name="twitter:card" value="summary" />
  <meta name="twitter:title" content="Luki - Dashboard" />
  <meta name="twitter:description" content="Luki is an easy-to-use bot with commands that can be used for fun, moderation, games, and more!" />
  <meta name="twitter:image" content='<%= bot.user.avatarURL() %>'>
  <meta name="theme-color" content="#551A8B">
  <meta property="og:site_name" value="dashboard.luki.xyz">
  <meta property="og:title" content="Luki - Dashboard" />
  <meta property="og:url" content="https://dashboard.luki.xyz" />
  <meta property="og:description" content="Luki is an easy-to-use bot with commands that can be used for fun, moderation, games, and more!" />
  <meta property="og:image" content='<%= bot.user.avatarURL() %>' />

  <!-- Bootstrap CSS -->
  <link href="https://bootswatch.com/4/cyborg/bootstrap.min.css" rel="stylesheet" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
  <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js" integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1" crossorigin="anonymous"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.19.1/moment.min.js" integrity="sha256-zG8v+NWiZxmjNi+CvUYnZwKtHzFtdO8cAKUIdB8+U9I=" crossorigin="anonymous"></script>
  <link rel="stylesheet" href="/public/dashboard.css" />
  <script src="https://cdn.datatables.net/v/bs4/dt-1.10.16/cr-1.4.1/r-2.2.0/datatables.min.js"></script>
  <link rel='icon' href='<%= bot.user.avatarURL %>'>
  <link rel="shortcut icon" href='<%= bot.user.avatarURL() %>'>
  <title>
    <%= bot.user.username %> - Dashboard
  </title>
</head>

<body>
  <a name="top"></a>
  <div class="nav-breadcrumb" style="width: 100%; background-color: #282828; margin-bottom: 40px;">
    <div class="container">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="/">Home</a></li>
      <%
        pathArray = path.split("/").slice(1);
        pathArray = pathArray.map(p=> {
          if(bot.guilds.has(p)) return bot.guilds.get(p).name;
          return p.toProperCase();
        });
        let builtPath = "";
        for(let i = 0; i < pathArray.length; i++) {
          builtPath += "/"+path.split("/").slice(1)[i]; %>
      <li class="breadcrumb-item<% if((i+1) === pathArray.length) {%> active"><%= pathArray[i] %><% } else { %>"><a href="<%= builtPath %>"><%= pathArray[i] %></a><% } %></li>
      <% } %>
    </ol>
  </div></div>
  <div class="container">